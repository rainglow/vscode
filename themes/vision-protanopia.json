{
  "name": "vision-colorblind",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#333333",
        "foreground": "#ffffff",
        "selectionForeground": "#ffffff",
        "findHighlight": "#ffe792",
        "findHighlightForeground": "#333333",
        "selectionBorder": "#ffffff",
        "bracketsForeground": "#ffffff",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#ffffff",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#119911"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#21c0fc"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#136efb"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "#f652ff"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#34f7a3"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#a2f702"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#f652ff"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#34f7a3"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#664e4d",
        "foreground": "#f78534",
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#664e4d",
        "foreground": "#f78534"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#21c0fc"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#f7346e"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#f7b83d"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#8FBE00A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#7b7b7b"
      }
    },
    {
      "scope": [
        "meta.property-value",
        "support.constant.property-value",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#21c0fc"
      }
    },
    {
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#fdba2c"
      }
    },
    {
      "scope": "meta.structure.dictionary.value.json string.quoted.double.json",
      "settings": {
        "foreground": "#21c0fc"
      }
    },
    {
      "name": "Function argument",
      "scope": "meta.property-name support.type.property-name",
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "name": "Function argument",
      "scope": "meta.property-value punctuation.separator.key-value",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "keyword.other.use",
        "keyword.other.function.use",
        "keyword.other.namespace",
        "keyword.other.new",
        "keyword.other.special-method",
        "keyword.other.unit",
        "keyword.other.use-as"
      ],
      "settings": {
        "foreground": "#f652ff"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "meta.use support.class.builtin",
        "meta.other.inherited-class support.class.builtin"
      ],
      "settings": {
        "foreground": "#f652ff",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.other",
      "settings": {
        "foreground": "#5fd",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Coffeescript Function argument",
      "scope": "variable.parameter.function.coffee",
      "settings": {
        "foreground": "#fdba2c",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown Titles",
      "scope": "entity.name.section.markdown",
      "settings": {
        "foreground": "#fdba2c"
      }
    },
    {
      "name": "Markdown Title Hash",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#136efb"
      }
    },
    {
      "name": "Markdown Raw",
      "scope": "markup.raw.inline.markdown",
      "settings": {
        "foreground": "#21c0fc"
      }
    },
    {
      "name": "Markdown bold stars",
      "scope": [
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "foreground": "#136efb"
      }
    },
    {
      "name": "Markdown link title braces",
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#136efb"
      }
    },
    {
      "name": "Markdown link braces",
      "scope": "punctuation.definition.metadata.markdown",
      "settings": {
        "foreground": "#136efb"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown",
        "meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#f652ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown bold/italic",
      "scope": ["markup.bold.markdown", "markup.italic.markdown"],
      "settings": {
        "foreground": "#f652ff"
      }
    },
    {
      "name": "Markdown bold/italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold/italic",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown pre",
      "scope": "markup.raw.block.markdown",
      "settings": {
        "foreground": "#664e4d"
      }
    },
    {
      "name": "GitGutter deleted",
      "scope": "markup.deleted.git_gutter",
      "settings": {
        "foreground": "#f87856"
      }
    },
    {
      "name": "GitGutter inserted",
      "scope": "markup.inserted.git_gutter",
      "settings": {
        "foreground": "#1ba2f7"
      }
    },
    {
      "name": "GitGutter changed",
      "scope": "markup.changed.git_gutter",
      "settings": {
        "foreground": "#81209e"
      }
    },
    {
      "name": "Template expressions.",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#ffffff"
      }
    }
  ],
